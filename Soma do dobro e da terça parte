num1, dobro, tercaParte: real
Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
Escreva ("Digite um número: ")
Leia (num1)
dobro <- num1 * 2
tercaParte <- num1 /3
Escreval ("Número:", num1)
Escreval("O dobro: ",dobro)
Escreval("Terça parte: ",tercaParte)
